{
    "appName": "learning-api",
    "instanceCount": 1,
    "notExposeAsWebApp": true,
    "forceSsl": true,
    "envVars": [
        {
            "key": "CODA_LEARNING_API_PORT",
            "value": "$CODA_LEARNING_API_PORT"
        },
        {
            "key": "CODA_FHIR_STORE_URL",
            "value": "$CODA_FHIR_STORE_URL"
        },
        {
            "key": "CODA_DICOM_STORE_URL",
            "value": "$CODA_DICOM_STORE_URL"
        },
        {
            "key": "CODA_SITE_CACHE_DB_HOST",
            "value": "$CODA_SITE_CACHE_DB_HOST"
        },
        {
            "key": "CODA_SITE_CACHE_DB_PASSWORD",
            "value": "$CODA_SITE_CACHE_DB_PASSWORD"
        },
        {
            "key": "CODA_SITE_CACHE_DB_PORT",
            "value": "$CODA_SITE_CACHE_DB_PORT"
        },
        {
            "key": "CODA_LEARNING_API_FHIR_STORE_CLIENT_ID",
            "value": "$CODA_LEARNING_API_FHIR_STORE_CLIENT_ID"
        },
        {
            "key": "CODA_LEARNING_API_FHIR_STORE_CLIENT_SECRET",
            "value": "$CODA_LEARNING_API_FHIR_STORE_CLIENT_SECRET"
        },
        {
            "key": "CODA_LEARNING_API_DICOM_STORE_CLIENT_ID",
            "value": "$CODA_LEARNING_API_DICOM_STORE_CLIENT_ID"
        },
        {
            "key": "CODA_LEARNING_API_DICOM_STORE_CLIENT_SECRET",
            "value": "$CODA_LEARNING_API_DICOM_STORE_CLIENT_SECRET"
        }
    ]
}